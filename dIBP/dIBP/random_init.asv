rng(20160209);

I = 10;
J = 20;
K = 4;
L = 6;
M = max(K, L);

[mu_u, mu_v] = bibetarnd(M, 1, 1, 1);
mu_u = mu_u(1:K); % truncated probability sequence
mu_v = mu_v(1:L);

mu_u_test = mu_u + normrnd(0, 0.001, K, 1);
mu_v_test = mu_v + normrnd(0, 0.001, L, 1);

U = matbinornd(mu_u, I, K);
V = matbinornd(mu_v, J, L);

probUV_mu(U, V, mu_u, mu_v) / probUV_mu(U, V, mu_u_test, mu_v_test);
